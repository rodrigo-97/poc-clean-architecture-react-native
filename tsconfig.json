{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@data/*": [
                "./src/data/*"
            ],
            "@domain/*": [
                "./src/domain/*"
            ],
            "@infra/*": [
                "./src/infra/*"
            ],
            "@presentation/*": [
                "./src/presentation/*"
            ]
        }
    }
}